    "start": "craco start",
    "build": "craco build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 22,
        "functions": 22,
        "lines": 22,
        "statements": 22
      }
    },
    "coverageReporters": [
      "cobertura",
      "json",
      "lcov",
      "text"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|konva|d3-.+?|internmap|rc-.+?|@babel/runtime).+(js|jsx)$"
    ]
  }
}
